{
  "name": "react-lightning-boilerplate",
  "version": "0.0.1",
  "description": "boilerplate for developing a Lightning Component With React",
  "private": true,
  "scripts": {
    "start": "set PORT=44365 && react-scripts start",
    "build": "node node_modules/webpack/bin/webpack.js --config webpack.prod.config.js --display-error-details",
